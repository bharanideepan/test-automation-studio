{
  "name": "test-automation-api",
  "description": "Rest api service for managing test automation",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && npm run coverage",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --no-notify src/",
    "start": "node lib/",
    "local": "set NODE_ENV=development && npm run compile && node lib/",
    "mocha": "set NODE_ENV=test && mocha --timeout 10000 --require ts-node/register --require source-map-support/register \"test/**/*.ts\" --recursive --exit",
    "compile": "tsc",
    "coverage": "nyc npm run mocha"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "index.d.ts",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@feathersjs/configuration": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.12",
    "@feathersjs/socketio": "^4.5.11",
    "@feathersjs/transport-commons": "^4.5.11",
    "@types/bull": "^3.15.8",
    "@types/dom-parser": "^0.1.1",
    "@types/dom-to-image": "^2.6.4",
    "@types/jsdom": "^16.2.14",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/parser": "^5.46.1",
    "adm-zip": "0.5.9",
    "aws-sdk": "^2.1069.0",
    "axios": "^0.26.0",
    "bull": "^4.6.2",
    "chai": "^4.3.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dom-parser": "^0.1.6",
    "dom-to-image": "^2.6.0",
    "feathers-sequelize": "6.3.2",
    "feathers-swagger": "^1.2.1",
    "helmet": "^4.6.0",
    "html-to-image": "^1.9.0",
    "html2canvas": "^1.4.1",
    "jsdom": "^19.0.0",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.10.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3-v2": "^2.2.1",
    "nodemailer": "^6.8.0",
    "nodemailer-html-to-text": "^3.2.0",
    "pg": "^8.7.1",
    "proxyquire": "^2.1.3",
    "puppeteer": "^15.1.1",
    "redis": "^4.6.6",
    "request": "^2.88.2",
    "sequelize": "6.15.1",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^4.3.0",
    "typescript": "4.6.3",
    "uuid": "^9.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
